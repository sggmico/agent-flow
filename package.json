{
  "name": "agent-flow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -F @agent-flow/web dev",
    "build": "pnpm -r build",
    "start": "pnpm -F @agent-flow/web start",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type-check": "pnpm -r type-check",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1"
}
